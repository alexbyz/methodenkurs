         <cei:body>
            <cei:idno id="222" old="514391">222</cei:idno>
            <cei:chDesc>
               <cei:abstract>
                  <cei:foreign>Pruder Irenfrid der Mer, maister des hauses ze dem Heiligen Geist ze Wienn, und aller
                     convent daselbes
                  </cei:foreign> verkaufen mit Handen ihres Bergherrn des Ritters 
                  <cei:foreign>hern Gerungs des Cholen, amptman des gotshauss ze Neumburch</cei:foreign>, ein halbes Joch Weingarten, 
                  <cei:foreign>der da leit ze Grinzing an dem Stainperig</cei:foreign> , . . 
                  <cei:foreign>und haisset der Pheffel</cei:foreign>, von dem man 5/4 Wein zu Bergrecht und 3 Helbling zu Voitrecht dient, um 125 lb dn.
                  
                  <cei:foreign>Chunraten dem Wiltwercher, purgermaister ze Wienn</cei:foreign>, und seiner Hausfrau 
                  <cei:foreign>Mergarten</cei:foreign>.
               </cei:abstract>
               <cei:issued>
                  <cei:placeName>Wien</cei:placeName>
                  <cei:date value="13400802">1340 VIII 02</cei:date>
               </cei:issued>
               <cei:witnessOrig>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00222_r.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00222_v.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:auth>
                     <cei:sealDesc>Drei anhangende Siegel, 1. spitzoval.Besiegelt von den Ausstellern und dem Bergherrn.</cei:sealDesc>
                  </cei:auth>
                  <cei:archIdentifier>Wiener Stadt- und Landesarchiv</cei:archIdentifier>
               </cei:witnessOrig>
               <cei:witListPar></cei:witListPar>
               <cei:diplomaticAnalysis>
                  <cei:quoteOriginaldatierung>ze Wienn, an sand Stephans abent.</cei:quoteOriginaldatierung>
               </cei:diplomaticAnalysis>
            </cei:chDesc>
