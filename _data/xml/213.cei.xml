         <cei:body>
            <cei:idno id="213" old="514382">213</cei:idno>
            <cei:chDesc>
               <cei:abstract>
                  <cei:foreign>Andre der Chrannest, pfarrer ze Ruprechtzhofen</cei:foreign> und Kaplan in seines Vaters Kapelle, 
                  <cei:foreign>die da leit in sant Stephans vreithof under dem neun Charner</cei:foreign>, gibt, mit Willen seiner Brüder seinem Kaplan 
                  <cei:foreign>hern Ulreichen zu sant Vergilii altar in derselben chappellen 7 lb dn. gelts</cei:foreign>, wovon 6 lb aus den 9 lb dn. genommen werden, die man von der 
                  <cei:foreign>padstuben bei dem Rotenturn under den Hafnern</cei:foreign> zu der genannten Kapelle dient, und 1 lb dn. anliegt auf 
                  <cei:foreign>Ru/edleins haus, Jacobs sun, des choler an dem Cholmarcht</cei:foreign>.
               </cei:abstract>
               <cei:issued>
                  <cei:date value="13400202">1340 II 02</cei:date>
               </cei:issued>
               <cei:witnessOrig>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00213_r.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00213_v.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:auth>
                     <cei:sealDesc>Drei anhangende Siegel.Besiegelt von dem Aussteller und seinen Brüdern 
                        <cei:legend>Jacob</cei:legend> und 
                        <cei:legend>Dietreich</cei:legend>.
                     </cei:sealDesc>
                  </cei:auth>
                  <cei:archIdentifier>Wiener Stadt- und Landesarchiv</cei:archIdentifier>
               </cei:witnessOrig>
