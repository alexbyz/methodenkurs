         <cei:body>
            <cei:idno id="208" old="514377">208</cei:idno>
            <cei:chDesc>
               <cei:abstract>
                  <cei:foreign>Marchart der Laurein, Ruger der Chombost, Wernhart, Rugers aidem in dem ampthof ze
                     Sufringe
                  </cei:foreign>, bestätigen, dass 
                  <cei:foreign>her Ulreich, hern Andres Schreiber des Chrannest, pharrer ze Ru/eprechtshofen, und
                     sein anwalt als verre gechlagt haben an irs egenanten herren stat und an der chapellen
                     statt datz sande Stephan ze Wienne, haisst die Gruft, hinz drin halben jeuchen weingarten,
                     ligent oberthalben Grinzinge an den Sibenstochen, umb 12 sh. dn. versessens purchrechts,
                     dass er mit recht an vier phunt hundert phunt phening ze rechtem zwispilde darauf
                     erlangt hat
                  </cei:foreign>, und verpflichten sich, mit Herrn Andreas, sobald er nach Wien komme, einen Vergleich
                  einzugehen.
               </cei:abstract>
               <cei:issued>
                  <cei:placeName>Wien</cei:placeName>
                  <cei:date value="13390309">1339 III 09</cei:date>
               </cei:issued>
               <cei:witnessOrig>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00208_r.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00208_v.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:auth>
                     <cei:sealDesc>Die Siegel fehlen.Besiegelt mit den Siegeln: 1. Marcharts, 2. 
                        <cei:legend>hern Jacobs des Chrannest</cei:legend>.
                     </cei:sealDesc>
                  </cei:auth>
                  <cei:nota>Indorsat: 15. Jahrhundert, 
                     <cei:foreign>Das ist ain pehabbrief von zwan weingarten zue Grinzinge, die haissent die Siebenstoke,
                        anno 1339
                     </cei:foreign>.
                  </cei:nota>
                  <cei:archIdentifier>Wiener Stadt- und Landesarchiv</cei:archIdentifier>
               </cei:witnessOrig>
               <cei:witListPar></cei:witListPar>
               <cei:diplomaticAnalysis>
                  <cei:p> 
                     <cei:persName>Andre Chrannest</cei:persName>
                     <cei:index>Kapelle der Chrannest</cei:index>
                     <cei:index>Gruft </cei:index>
                     <cei:persName>Jacob Chrannest</cei:persName>
                  </cei:p>
                  <cei:quoteOriginaldatierung>ze Wienne, des eritags vor sande Greiorgen tage.</cei:quoteOriginaldatierung>
               </cei:diplomaticAnalysis>
