         <cei:body>
            <cei:idno id="220" old="514389">220</cei:idno>
            <cei:chDesc>
               <cei:abstract>
                  <cei:foreign>Elspet, Berngers witibe</cei:foreign>, verkauft mit Handen ihres Grundherrn 
                  <cei:foreign>des erbern priesters hern Otten des Gnemhertelins, chaplan Unser Vrowen auf der Stetten
                     ze Wienne
                  </cei:foreign>, ½ 
                  <cei:foreign>lb dn. gelts purchrechts</cei:foreign>, das sie auf 
                  <cei:foreign>Ulreichs haus, des slosser, des Paires, das da leit pei sand Stephan zenahst Eberharts
                     hause, des slosser, davon man dient alle jar ze gruntrecht 3 dn
                  </cei:foreign>., gehabt hatte, um 3 ½ lb dn. 
                  <cei:foreign>hern Haunolten dem Schu/eheler, purger ze Wienne</cei:foreign>.
               </cei:abstract>
               <cei:issued>
                  <cei:placeName>Wien</cei:placeName>
                  <cei:date value="13400615">1340 VI 15</cei:date>
               </cei:issued>
               <cei:witnessOrig>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00220_r.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:figure>
                     <cei:graphic url="WStLA-HA-Urkunden_00220_v.jpg"></cei:graphic>
                  </cei:figure>
                  <cei:auth>
                     <cei:sealDesc>Zwei anhangende Siegel, roth u. K., 1. spitzoval, 2. rund.Besiegelt mit den Siegeln:
                        1. des Grundherrn, 2. 
                        <cei:legend>Stephans des Chez/zz/zeler, hern Antonii aidem auf der Widem</cei:legend>.
                     </cei:sealDesc>
                  </cei:auth>
                  <cei:archIdentifier>Wiener Stadt- und Landesarchiv</cei:archIdentifier>
               </cei:witnessOrig>
               <cei:witListPar></cei:witListPar>
               <cei:diplomaticAnalysis>
                  <cei:quoteOriginaldatierung>ze Wienne, an Gotes Leichnames tage.</cei:quoteOriginaldatierung>
                  <cei:listBiblEdition>
                     <cei:bibl>Jahrbuch der kais. Kunstsamml. 16b, no 12689 Regest.</cei:bibl>
                  </cei:listBiblEdition>
                  <cei:p>
                     <cei:bibl>Auf angeheftetem Pergamentstreifen: Das ist der prief um ½ phunt geltes auf des slosser
                        haus vor sand Stephan.
                     </cei:bibl>
                  </cei:p>
                  <cei:p>
                     <cei:index>Haus des Eberhard Schlossers</cei:index>  
                     <cei:index>Haus des Ulreich Payr</cei:index>  
                     <cei:placeName>bei St. Stephan</cei:placeName>
                  </cei:p>
               </cei:diplomaticAnalysis>
               <cei:lang_MOM></cei:lang_MOM>
            </cei:chDesc>
